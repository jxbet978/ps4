# Configuración del servidor
PORT=3000

# Telegram Bot Configuration
# Obtén tu token en: https://t.me/BotFather
TELEGRAM_BOT_TOKEN=tu_bot_token_aqui

# ID del chat o grupo donde recibirás los mensajes
# Para obtenerlo, envía un mensaje al bot y visita: https://api.telegram.org/botTU_TOKEN/getUpdates
TELEGRAM_CHAT_ID=tu_chat_id_aqui

# Configuración de sesión (en milisegundos)
SESSION_EXPIRY_TIME=1800000
